{
  "name": "window-commands-extension",
  "version": "1.0.0",
  "description": "GNOME Shell extension for window management commands",
  "type": "module",
  "scripts": {
    "build": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --format=esm --target=firefox102 --external:gi://* --external:resource://*",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "@girs/gio-2.0": "^2.86.0-4.0.0-beta.38",
    "@girs/gjs": "^4.0.0-beta.38",
    "@girs/glib-2.0": "^2.86.0-4.0.0-beta.38",
    "@girs/gnome-shell": "^49.1.0",
    "@girs/meta-17": "^17.0.0-4.0.0-beta.38",
    "@girs/shell-17": "^17.0.0-4.0.0-beta.38",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0"
  }
}
